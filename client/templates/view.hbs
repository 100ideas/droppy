<div class="view">
  <div class="view-nav">
    <div class="view-nav-left">
      <button class="af button tip tip-se" aria-label="Upload files from disk"><svg class="add-file"><use xlink:href="#i-add-file"/></svg></button>
      <button class="ad button tip tip-s" aria-label="Upload folder from disk"><svg class="add-folder"><use xlink:href="#i-add-folder"/></svg></button>
      <button class="cf button tip tip-s" aria-label="Create file"><svg class="create-file"><use xlink:href="#i-create-file"/></svg></button>
      <button class="cd button tip tip-s" aria-label="Create folder"><svg class="create-folder"><use xlink:href="#i-create-folder"/></svg></button>
    </div>
    <div class="view-nav-right">
      <button class="newview button tip tip-s" aria-label="Create new View"><svg class="window"><use xlink:href="#i-window"/></svg></button>
      <button class="prefs button tip tip-s" aria-label="Preferences"><svg class="cog"><use xlink:href="#i-cog"/></svg></button>
      <button class="about button tip tip-s" aria-label="About"><svg class="info"><use xlink:href="#i-info"/></svg></button>
      <button class="logout button tip tip-sw" aria-label="Sign out"><svg class="signout"><use xlink:href="#i-signout"/></svg></button>
    </div>
  </div>
  <ul class="path"></ul>
  <div class="content-container"><div class="content"></div></div>
  <div class="dropzone">
    <svg></svg>
  </div>
  <div class="info-box">
    <div class="icon">{{{svg "link"}}}</div>
    <span></span>
    <div class="link-out mousetrap"></div>
    <div class="link-options">
      <div class="copy-link tip tip-nw" aria-label="Copy to clipboard">
        {{{svg "copy"}}}<div>Copy</div>
      </div>
      <div class="dl-link checked tip tip-sw" aria-label="Trigger a download when opened">
        {{{svg "check"}}}<div>Is DL</div>
      </div>
    </div>
  </div>
  <div class="paste-button">
    {{{svg "paste"}}}
    <span>Paste here</span>
    {{{svg "triangle"}}}
  </div>
  <div class="audio-bar">
    <div class="audio-icon volume">{{{svg "volume-medium"}}}</div>
    <div class="volume-slider">
      <div class="volume-slider-inner"></div>
    </div>
    <div class="audio-icon previous">{{{svg "previous"}}}</div>
    <div class="audio-icon pause-play">{{{svg "pause"}}}</div>
    <div class="audio-icon stop">{{{svg "stop"}}}</div>
    <div class="audio-icon shuffle">{{{svg "shuffle"}}}</div>
    <div class="audio-icon next">{{{svg "next"}}}</div>
    <div class="audio-title"></div>
    <div class="time">
      <span class="time-cur"></span>
      <span class="time-sep"> / </span>
      <span class="time-max"></span>
    </div>
    <div class="seekbar">
      <div class="seekbar-played"></div>
      <div class="seekbar-loaded"></div>
    </div>
    <audio class="audio-player" preload="none"></audio>
  </div>
</div>
